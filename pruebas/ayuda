CREATE INDEX IDX_CUSTOMER_LAST_NAME
ON Customer (Last_Name);
CREATE INDEX "NOMBRE_ÍNDICE" ON "NOMBRE_TABLA" (NOMBRE_COLUMNA);

CREATE TABLE Costos_historial_medico ( id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, id_historial_medico INT NOT NULL, evento VARCHAR(70) NOT NULL, total DEC(7,2) NOT NULL, id_evento_historial INT NOT NULL, INDEX (id_evento_historial), INDEX (id_historial_medico), FOREIGN KEY (id_evento_historial) REFERENCES Historial_medico(id), FOREIGN KEY (id_historial_medico) REFERENCES No_historial_medico(id) );


