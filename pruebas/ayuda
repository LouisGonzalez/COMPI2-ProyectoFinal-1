    CUERPO_METODO ::= VARIABLES CUERPO_METODO
    |                 FOR CUERPO_METODO
    |                 WHILE CUERPO_METODO
    |                 IF CUERPO_METODO 
    |                 BORRAR CUERPO_METODO
    |                 MENSAJES CUERPO_METODO
    |                 METODO CUERPO_METODO {: /*PENDIENTE PRODUCCION LECTURA_DATOS*/ :}
    |                 condPass
;


CONDICION -> genera clavo


%ignorecase

    CONTENIDO ::= VARIABLES CONTENIDO
    |             ASIGNACIONES CONTENIDO
    |             IF CONTENIDO 
    |             SELECT CONTENIDO
    |             DO_WHILE CONTENIDO
    |             WHILE CONTENIDO
    |             FOR CONTENIDO
    |             MENSAJE CONTENIDO
    |             LECTURA_DATO CONTENIDO
    |
;


