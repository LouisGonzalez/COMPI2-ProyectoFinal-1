public Function Factorial(N As Integer) as Integer

    Dim Fact As Integer, Num As Integer
    Num = N
    N = intinput("ingrese un numero")
    If N = 0 Then
	Fact = 1
	Dim h As decimal
	h = 1 + 2.5
    Else
        Fact = 1
        Do
            Fact = Fact * N
	    N = N - 1
       Loop While N > 1
	Fact = Fact
    End If
    return '2'	
End Function



